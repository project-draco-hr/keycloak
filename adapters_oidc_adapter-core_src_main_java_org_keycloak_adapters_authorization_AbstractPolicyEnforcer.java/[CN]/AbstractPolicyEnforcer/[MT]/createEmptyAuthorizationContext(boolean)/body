{
  return new AuthorizationContext(){
    @Override public boolean hasPermission(    String resourceName,    String scopeName){
      return granted;
    }
    @Override public boolean hasResourcePermission(    String resourceName){
      return granted;
    }
    @Override public boolean hasScopePermission(    String scopeName){
      return granted;
    }
    @Override public List<Permission> getPermissions(){
      return Collections.EMPTY_LIST;
    }
    @Override public boolean isGranted(){
      return granted;
    }
  }
;
}
