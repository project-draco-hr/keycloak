{
  Event clone=new Event();
  clone.time=time;
  clone.event=event;
  clone.realmId=realmId;
  clone.clientId=clientId;
  clone.userId=userId;
  clone.sessionId=sessionId;
  clone.ipAddress=ipAddress;
  clone.error=error;
  clone.details=details != null ? new HashMap<String,String>(details) : null;
  return clone;
}
