{
  DBCursor cur=audit.find(query);
  if (firstResult != null) {
    cur.skip(firstResult);
  }
  if (maxResults != null) {
    cur.limit(maxResults);
  }
  List<Event> events=new LinkedList<Event>();
  while (cur.hasNext()) {
    events.add(MongoAuditProvider.convert((BasicDBObject)cur.next()));
  }
  return events;
}
