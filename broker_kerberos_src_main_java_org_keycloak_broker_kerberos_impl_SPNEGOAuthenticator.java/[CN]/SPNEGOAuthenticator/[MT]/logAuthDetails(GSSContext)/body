{
  if (logger.isInfoEnabled()) {
    String message=new StringBuilder("SPNEGO Security context accepted with token: " + responseToken).append(", established: " + gssContext.isEstablished()).append(", credDelegState: " + gssContext.getCredDelegState()).append(", mutualAuthState: " + gssContext.getMutualAuthState()).append(", lifetime: " + gssContext.getLifetime()).append(", confState: " + gssContext.getConfState()).append(", integState: " + gssContext.getIntegState()).append(", srcName: " + gssContext.getSrcName()).append(", targName: " + gssContext.getTargName()).toString();
    logger.info(message);
  }
}
