{
  if (log.isDebugEnabled()) {
    String message=new StringBuilder("SPNEGO Security context accepted with token: " + responseToken).append(", established: ").append(gssContext.isEstablished()).append(", credDelegState: ").append(gssContext.getCredDelegState()).append(", mutualAuthState: ").append(gssContext.getMutualAuthState()).append(", lifetime: ").append(gssContext.getLifetime()).append(", confState: ").append(gssContext.getConfState()).append(", integState: ").append(gssContext.getIntegState()).append(", srcName: ").append(gssContext.getSrcName()).append(", targName: ").append(gssContext.getTargName()).toString();
    log.debug(message);
  }
}
