{
  return SimpleHttp.doPost(getConfig().getTokenUrl()).param(OAUTH2_PARAMETER_CODE,authorizationCode).param(OAUTH2_PARAMETER_CLIENT_ID,getConfig().getClientId()).param(OAUTH2_PARAMETER_CLIENT_SECRET,getConfig().getClientSecret()).param(OAUTH2_PARAMETER_REDIRECT_URI,uriInfo.getAbsolutePath().toString()).param(OAUTH2_PARAMETER_GRANT_TYPE,OAUTH2_GRANT_TYPE_AUTHORIZATION_CODE);
}
