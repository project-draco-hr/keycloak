{
  s=s.replace('-','+');
  s=s.replace('_','/');
switch (s.length() % 4) {
case 0:
    break;
case 2:
  s+="==";
break;
case 3:
s+="=";
break;
default :
throw new RuntimeException("Illegal base64url string!");
}
try {
return Base64.decode(s);
}
 catch (Exception e) {
throw new RuntimeException(e);
}
}
