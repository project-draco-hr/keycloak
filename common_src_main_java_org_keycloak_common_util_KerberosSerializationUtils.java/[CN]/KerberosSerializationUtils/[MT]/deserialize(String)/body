{
  byte[] bytes=Base64.decode(serialized);
  ByteArrayInputStream bis=new ByteArrayInputStream(bytes);
  ObjectInput in=null;
  try {
    in=new ObjectInputStream(bis);
    return in.readObject();
  }
  finally {
    try {
      if (in != null) {
        in.close();
      }
    }
 catch (    IOException ex) {
      ex.printStackTrace();
    }
  }
}
