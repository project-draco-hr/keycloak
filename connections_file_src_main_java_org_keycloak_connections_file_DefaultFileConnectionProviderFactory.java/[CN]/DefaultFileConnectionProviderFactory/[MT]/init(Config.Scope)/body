{
  String fileName=config.get("fileName");
  if (fileName == null) {
    fileName="keycloak-model.json";
  }
  String directory=config.get("directory");
  if (directory == null) {
    directory=System.getProperty("jboss.server.data.dir");
  }
  if (directory == null) {
    directory=".";
  }
  kcdata=new File(directory,fileName);
}
