{
  KeyStore trustStore=KeyStore.getInstance(KeyStore.getDefaultType());
  InputStream trustStream=(filename.startsWith(PROTOCOL_CLASSPATH)) ? KeystoreUtil.class.getResourceAsStream(filename.replace(PROTOCOL_CLASSPATH,"")) : new FileInputStream(new File(filename));
  trustStore.load(trustStream,password.toCharArray());
  trustStream.close();
  return trustStore;
}
