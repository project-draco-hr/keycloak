{
  token=new SkeletonKeyToken();
  token.principal("CN=Client").audience("domain").addAccess("service").addRole("admin").verifyCaller(true);
  byte[] tokenBytes=JsonSerialization.toByteArray(token,false);
  String encoded=new JWSBuilder().content(tokenBytes).rsa256(idpPair.getPrivate());
  SkeletonKeyToken v=null;
  try {
    v=RSATokenVerifier.verifyToken(encoded,metadata);
  }
 catch (  VerificationException ignored) {
    System.out.println(ignored.getMessage());
  }
}
