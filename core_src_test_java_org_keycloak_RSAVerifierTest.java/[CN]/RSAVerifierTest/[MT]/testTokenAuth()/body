{
  token=new SkeletonKeyToken();
  token.subject("CN=Client").audience("domain").addAccess("service").addRole("admin").verifyCaller(true);
  String encoded=new JWSBuilder().jsonContent(token).rsa256(idpPair.getPrivate());
  SkeletonKeyToken v=null;
  try {
    v=verifySkeletonKeyToken(encoded);
  }
 catch (  VerificationException ignored) {
    System.out.println(ignored.getMessage());
  }
}
