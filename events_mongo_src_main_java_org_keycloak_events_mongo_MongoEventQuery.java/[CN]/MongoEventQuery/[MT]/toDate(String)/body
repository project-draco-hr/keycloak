{
  SimpleDateFormat df=new SimpleDateFormat("yyyy-MM-dd");
  Long to=null;
  try {
    to=df.parse(toDate).getTime();
  }
 catch (  ParseException e) {
    e.printStackTrace();
  }
  BasicDBObject time=query.containsField("time") ? (BasicDBObject)query.get("time") : new BasicDBObject();
  time.append("$lte",to);
  query.put("time",time);
  return this;
}
