def unisymbol(codePoint):
    if ((codePoint >= 0) and (codePoint <= 65535)):
        return unichr(codePoint)
    elif ((codePoint >= 65536) and (codePoint <= 1114111)):
        highSurrogate = (int(((codePoint - 65536) / 1024)) + 55296)
        lowSurrogate = (int(((codePoint - 65536) % 1024)) + 56320)
        return (unichr(highSurrogate) + unichr(lowSurrogate))
    else:
        return 'Error'
