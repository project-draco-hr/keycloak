{
  Iterator<Event> itr=this.events.iterator();
  while (itr.hasNext()) {
    Event next=itr.next();
    boolean include=false;
    for (    EventType e : events) {
      if (next.getEvent().equals(e)) {
        include=true;
        break;
      }
    }
    if (!include) {
      itr.remove();
    }
  }
  return this;
}
