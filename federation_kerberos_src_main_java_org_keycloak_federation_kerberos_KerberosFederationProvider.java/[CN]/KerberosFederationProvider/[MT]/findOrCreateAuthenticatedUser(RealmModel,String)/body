{
  UserModel user=session.userStorage().getUserByUsername(username,realm);
  if (user != null) {
    logger.debug("Kerberos authenticated user " + username + " found in Keycloak storage");
    if (!isValid(user)) {
      throw new IllegalStateException("User with username " + username + " already exists, but is not linked to provider ["+ model.getDisplayName()+ "] or kerberos principal is not correct. Kerberos principal on user is: "+ user.getAttribute(KERBEROS_PRINCIPAL));
    }
    return proxy(user);
  }
 else {
    return importUserToKeycloak(realm,username);
  }
}
