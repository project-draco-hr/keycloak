{
  UserModel user=session.userStorage().getUserByUsername(username,realm);
  if (user != null) {
    logger.debug("Kerberos authenticated user " + username + " found in Keycloak storage");
    if (!isValid(user)) {
      throw new IllegalStateException("User with username " + username + " already exists, but is not linked to provider ["+ model.getDisplayName()+ "] or LDAP_ID is not correct. LDAP_ID on user is: "+ user.getAttribute(LDAP_ID));
    }
    return proxy(user);
  }
 else {
    return getUserByUsername(realm,username);
  }
}
