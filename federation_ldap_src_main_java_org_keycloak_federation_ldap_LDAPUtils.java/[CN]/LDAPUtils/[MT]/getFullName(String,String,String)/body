{
  String fullName;
  if (firstName != null && lastName != null) {
    fullName=firstName + " " + lastName;
  }
 else   if (firstName != null && firstName.trim().length() > 0) {
    fullName=firstName;
  }
 else {
    fullName=lastName;
  }
  if (fullName == null || fullName.trim().length() == 0) {
    fullName=username;
  }
  return fullName;
}
