{
  Object array=Array.newInstance(arrayType,0);
  test(iface,proxy,method,array,array);
  Random r=new Random();
  int l=(r.nextInt() % 100);
  array=Array.newInstance(arrayType,(l < 0 ? -l : l) + 15);
  if (null != content)   Arrays.fill((Object[])array,content);
  test(iface,proxy,method,array,array);
}
