{
  if (locale == null) {
    return;
  }
  boolean secure=realm.getSslRequired().isRequired(uriInfo.getRequestUri().getHost());
  builder.cookie(new NewCookie(LocaleHelper.LOCALE_COOKIE,locale.toLanguageTag(),keycloakLocaleCookiePath,null,null,31536000,secure),new NewCookie(LocaleHelper.NG_LOCALE_COOKIE,"%22" + locale.toLanguageTag() + "%22",ngTranslateLocaleCookiePath,null,null,31536000,secure));
}
