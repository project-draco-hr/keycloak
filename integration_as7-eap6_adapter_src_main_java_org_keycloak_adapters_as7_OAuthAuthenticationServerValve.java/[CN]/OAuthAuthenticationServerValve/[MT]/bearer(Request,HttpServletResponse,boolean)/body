{
  if (request.getHeader("Authorization") != null) {
    CatalinaBearerTokenAuthenticator bearer=new CatalinaBearerTokenAuthenticator(resourceMetadata,true,false);
    try {
      if (bearer.login(request,response)) {
        return true;
      }
    }
 catch (    LoginException e) {
    }
  }
  return false;
}
