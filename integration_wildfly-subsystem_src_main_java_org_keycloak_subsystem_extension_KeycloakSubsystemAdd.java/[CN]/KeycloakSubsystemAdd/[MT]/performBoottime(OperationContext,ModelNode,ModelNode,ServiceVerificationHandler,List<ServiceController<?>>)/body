{
  context.addStep(new AbstractDeploymentChainStep(){
    @Override protected void execute(    DeploymentProcessorTarget processorTarget){
      processorTarget.addDeploymentProcessor(KeycloakExtension.SUBSYSTEM_NAME,Phase.DEPENDENCIES,0,new KeycloakDependencyProcessor());
      processorTarget.addDeploymentProcessor(KeycloakExtension.SUBSYSTEM_NAME,KeycloakAdapterConfigDeploymentProcessor.PHASE,KeycloakAdapterConfigDeploymentProcessor.PRIORITY,new KeycloakAdapterConfigDeploymentProcessor());
    }
  }
,OperationContext.Stage.RUNTIME);
}
