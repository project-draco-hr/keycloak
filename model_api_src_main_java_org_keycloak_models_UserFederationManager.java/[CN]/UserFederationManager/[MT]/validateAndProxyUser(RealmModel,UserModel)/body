{
  UserFederationProvider link=getFederationLink(realm,user);
  if (link != null) {
    UserModel validatedProxyUser=link.validateAndProxy(realm,user);
    if (validatedProxyUser != null) {
      return validatedProxyUser;
    }
 else {
      deleteInvalidUser(realm,user);
      return null;
    }
  }
  return user;
}
