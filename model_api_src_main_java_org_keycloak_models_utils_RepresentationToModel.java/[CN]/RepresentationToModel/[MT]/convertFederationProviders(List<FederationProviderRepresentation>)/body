{
  List<UserFederationProviderModel> result=new ArrayList<UserFederationProviderModel>();
  for (  FederationProviderRepresentation representation : providers) {
    UserFederationProviderModel model=new UserFederationProviderModel(representation.getId(),representation.getProviderName(),representation.getConfig());
    result.add(model);
  }
  return result;
}
