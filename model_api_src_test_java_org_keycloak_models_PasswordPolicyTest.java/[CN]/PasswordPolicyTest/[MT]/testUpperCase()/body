{
  PasswordPolicy policy=new PasswordPolicy("upperCase");
  Assert.assertNotNull(policy.validate("abcd1234"));
  Assert.assertNull(policy.validate("abCd1234"));
  policy=new PasswordPolicy("upperCase(2)");
  Assert.assertNotNull(policy.validate("abCd1234"));
  Assert.assertNull(policy.validate("AbCd1234"));
}
