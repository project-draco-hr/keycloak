{
  if (list.isEmpty())   return list;
  Collections.sort(list);
  int first=(firstResult <= 0) ? 0 : firstResult;
  int last=first + maxResults;
  if ((maxResults > list.size() - first) || (last > list.size())) {
    last=list.size();
  }
  if (maxResults <= 0) {
    last=list.size();
  }
  return list.subList(first,last);
}
