{
  if (app == null) {
    realm.setAdminAppId(null);
    this.masterAdminApp=null;
  }
 else {
    String appId=app.getId();
    if (appId == null) {
      throw new IllegalStateException("Master Admin app not initialized.");
    }
    realm.setAdminAppId(appId);
    this.masterAdminApp=app;
  }
}
