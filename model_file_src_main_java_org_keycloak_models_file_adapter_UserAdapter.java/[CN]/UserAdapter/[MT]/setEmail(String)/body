{
  if (email == null) {
    user.setEmail(email);
    return;
  }
  if (email.equals(getEmail()))   return;
  RealmAdapter realmAdapter=(RealmAdapter)realm;
  if (realmAdapter.hasUserWithEmail(email))   throw new ModelDuplicateException("User with email address " + email + " already exists.");
  user.setEmail(email);
}
