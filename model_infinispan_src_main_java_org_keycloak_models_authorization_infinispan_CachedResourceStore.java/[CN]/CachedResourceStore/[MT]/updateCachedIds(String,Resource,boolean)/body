{
  List<String> cached=this.cache.get(cacheKey);
  if (cached == null) {
    if (!create) {
      return;
    }
    cached=new ArrayList<>();
    this.cache.put(getResourceOwnerCacheKey(resource.getOwner()),cached);
  }
  if (cached != null && !cached.contains(resource.getId())) {
    cached.add(resource.getId());
  }
}
