{
  if (!event.isPre()) {
    Object object=event.getValue();
    if (object != null) {
      if (object instanceof CachedRealm) {
        CachedRealm realm=(CachedRealm)object;
        realmLookup.put(realm.getName(),realm.getId());
        log.tracev("Realm added realm={0}",realm.getName());
      }
    }
  }
}
