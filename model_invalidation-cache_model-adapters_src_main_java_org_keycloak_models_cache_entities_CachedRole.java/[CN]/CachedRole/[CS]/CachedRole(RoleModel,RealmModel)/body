{
  composite=model.isComposite();
  description=model.getDescription();
  id=model.getId();
  name=model.getName();
  scopeParamRequired=model.isScopeParamRequired();
  this.realm=realm.getId();
  if (composite) {
    for (    RoleModel child : model.getComposites()) {
      composites.add(child.getId());
    }
  }
}
