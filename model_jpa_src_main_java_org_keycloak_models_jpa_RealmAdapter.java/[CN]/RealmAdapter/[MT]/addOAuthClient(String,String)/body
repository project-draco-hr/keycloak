{
  OAuthClientEntity data=new OAuthClientEntity();
  data.setId(id);
  data.setEnabled(true);
  data.setName(name);
  data.setRealm(realm);
  em.persist(data);
  em.flush();
  OAuthClientModel model=new OAuthClientAdapter(this,data,em);
  addDefaultClientProtocolMappers(model);
  em.flush();
  return model;
}
