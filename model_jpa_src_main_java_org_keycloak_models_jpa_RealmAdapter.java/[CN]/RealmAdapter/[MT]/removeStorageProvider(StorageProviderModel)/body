{
  Iterator<StorageProviderEntity> it=realm.getStorageProviders().iterator();
  while (it.hasNext()) {
    StorageProviderEntity entity=it.next();
    if (entity.getId().equals(provider.getId())) {
      session.users().preRemove(this,provider);
      it.remove();
      em.remove(entity);
      return;
    }
  }
}
