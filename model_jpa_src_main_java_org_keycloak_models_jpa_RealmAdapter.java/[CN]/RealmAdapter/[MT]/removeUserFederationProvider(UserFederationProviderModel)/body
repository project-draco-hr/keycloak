{
  Iterator<UserFederationProviderEntity> it=realm.getUserFederationProviders().iterator();
  while (it.hasNext()) {
    UserFederationProviderEntity entity=it.next();
    if (entity.getId().equals(provider.getId())) {
      session.users().preRemove(this,provider);
      it.remove();
      em.remove(entity);
      return;
    }
  }
}
