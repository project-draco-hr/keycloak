{
  OfflineClientSessionEntity found=null;
  for (  OfflineClientSessionEntity session : user.getOfflineClientSessions()) {
    if (session.getClientSessionId().equals(clientSessionId)) {
      found=session;
      break;
    }
  }
  if (found == null) {
    return false;
  }
 else {
    user.getOfflineClientSessions().remove(found);
    em.remove(found);
    em.flush();
    return true;
  }
}
