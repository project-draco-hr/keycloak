{
  TypedQuery<FederatedUserCredentialEntity> query=em.createNamedQuery("federatedUserCredentialByNameAndType",FederatedUserCredentialEntity.class).setParameter("type",type).setParameter("device",name).setParameter("userId",user.getId());
  List<FederatedUserCredentialEntity> results=query.getResultList();
  if (results.isEmpty())   return null;
  return toModel(results.get(0));
}
