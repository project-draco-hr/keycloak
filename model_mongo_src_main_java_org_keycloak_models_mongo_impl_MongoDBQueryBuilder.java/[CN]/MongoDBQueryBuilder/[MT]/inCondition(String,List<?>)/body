{
  if (values == null) {
    values=new LinkedList<Object>();
  }
  if ("_id".equals(name)) {
    List<ObjectId> objIds=new ArrayList<ObjectId>();
    for (    Object object : values) {
      ObjectId objectId=new ObjectId(object.toString());
      objIds.add(objectId);
    }
    values=objIds;
  }
  BasicDBObject inObject=new BasicDBObject("$in",values);
  put(name,inObject);
  return this;
}
