{
  Set<RoleModel> allRoles=getRoleMappings(user);
  Set<RoleModel> realmRoles=new HashSet<RoleModel>();
  for (  RoleModel role : allRoles) {
    RoleEntity roleEntity=((RoleAdapter)role).getRole();
    if (getId().equals(roleEntity.getRealmId())) {
      realmRoles.add(role);
    }
  }
  return realmRoles;
}
