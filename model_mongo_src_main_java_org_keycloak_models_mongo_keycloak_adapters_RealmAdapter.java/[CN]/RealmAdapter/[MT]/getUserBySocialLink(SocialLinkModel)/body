{
  DBObject query=new QueryBuilder().and("socialLinks.socialProvider").is(socialLink.getSocialProvider()).and("socialLinks.socialUsername").is(socialLink.getSocialUsername()).and("realmId").is(getId()).get();
  UserEntity userEntity=getMongoStore().loadSingleEntity(UserEntity.class,query,invocationContext);
  return userEntity == null ? null : new UserAdapter(userEntity,invocationContext);
}
