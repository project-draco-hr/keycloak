{
  Attribute<Serializable> a=user.getAttribute(name);
  Set<Serializable> s=new HashSet<Serializable>();
  if (a != null) {
    Serializable o=a.getValue();
    if (o instanceof Serializable[]) {
      for (      Serializable t : (Serializable[])o) {
        s.add(t);
      }
    }
 else {
      s.add(o);
    }
  }
  return (Set<T>)s;
}
