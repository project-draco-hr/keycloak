{
  log.tracev("Adding cache operation: {0} on {1}",CacheOperation.REPLACE,key);
  CacheTask current=tasks.get(key);
  if (current != null) {
switch (current.operation) {
case ADD:
case REPLACE:
      current.value=value;
    return;
case REMOVE:
  return;
}
}
 else {
tasks.put(key,new CacheTask(cache,CacheOperation.REPLACE,key,value));
}
}
