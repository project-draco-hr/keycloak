{
  List<Integer> result=new ArrayList<>();
  boolean remaining=true;
  int next=0;
  while (remaining && result.size() < segmentCount) {
    next=getNextUnfinishedSegmentFromIndex(next);
    if (next == -1) {
      remaining=false;
    }
 else {
      result.add(next);
      next++;
    }
  }
  return result;
}
