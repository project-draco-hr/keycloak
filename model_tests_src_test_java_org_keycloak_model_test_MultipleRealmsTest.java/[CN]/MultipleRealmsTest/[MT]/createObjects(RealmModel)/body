{
  ApplicationModel app1=realm.addApplication("app1");
  realm.addApplication("app2");
  realm.addUser("user1");
  realm.addUser("user2");
  realm.addRole("role1");
  realm.addRole("role2");
  app1.addRole("app1Role1");
  realm.addScopeMapping(app1,realm.getRole("role1"));
  realm.addOAuthClient("cl1");
}
