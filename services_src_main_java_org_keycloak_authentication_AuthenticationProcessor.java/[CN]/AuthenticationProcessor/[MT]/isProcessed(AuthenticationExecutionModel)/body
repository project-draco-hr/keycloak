{
  if (model.isDisabled())   return true;
  UserSessionModel.AuthenticatorStatus status=clientSession.getAuthenticators().get(model.getId());
  if (status == null)   return false;
  return status == UserSessionModel.AuthenticatorStatus.SUCCESS || status == UserSessionModel.AuthenticatorStatus.SKIPPED || status == UserSessionModel.AuthenticatorStatus.ATTEMPTED || status == UserSessionModel.AuthenticatorStatus.SETUP_REQUIRED;
}
