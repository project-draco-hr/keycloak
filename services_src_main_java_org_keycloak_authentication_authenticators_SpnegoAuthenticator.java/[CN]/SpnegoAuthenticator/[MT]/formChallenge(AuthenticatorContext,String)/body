{
  String accessCode=context.generateAccessCode();
  URI action=getActionUrl(context,accessCode,KERBEROS_DISABLED);
  return context.getSession().getProvider(LoginFormsProvider.class).setClientSessionCode(accessCode).setActionUri(action).setStatus(Response.Status.UNAUTHORIZED).setResponseHeader(HttpHeaders.WWW_AUTHENTICATE,negotiateHeader).setUser(context.getUser()).createForm("bypass_kerberos.ftl",new HashMap<String,Object>());
}
