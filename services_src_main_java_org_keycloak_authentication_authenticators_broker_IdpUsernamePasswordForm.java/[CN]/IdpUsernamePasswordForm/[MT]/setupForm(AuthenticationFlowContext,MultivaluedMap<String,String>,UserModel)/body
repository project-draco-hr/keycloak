{
  SerializedBrokeredIdentityContext serializedCtx=SerializedBrokeredIdentityContext.readFromClientSession(context.getClientSession());
  if (serializedCtx == null) {
    throw new AuthenticationFlowException("Not found serialized context in clientSession",AuthenticationFlowError.IDENTITY_PROVIDER_ERROR);
  }
  formData.add(AuthenticationManager.FORM_USERNAME,existingUser.getUsername());
  return context.form().setFormData(formData).setAttribute(LoginFormsProvider.USERNAME_EDIT_DISABLED,true).setSuccess(Messages.FEDERATED_IDENTITY_CONFIRM_REAUTHENTICATE_MESSAGE,existingUser.getUsername(),serializedCtx.getIdentityProviderId());
}
