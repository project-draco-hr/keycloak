{
  super.validateConfiguration(session,model);
  ConfigurationValidationHelper.check(model).checkSingle(KEYSTORE_PROPERTY,true).checkSingle(KEYSTORE_PASSWORD_PROPERTY,true).checkSingle(KEY_ALIAS_PROPERTY,true).checkSingle(KEY_PASSWORD_PROPERTY,true).checkSingle(CERTIFICATE_ALIAS_PROPERTY,false);
  try {
    new JavaKeystoreKeyProvider(session.getContext().getRealm(),model).loadKeys(session.getContext().getRealm(),model);
  }
 catch (  Throwable t) {
    throw new ComponentValidationException("Failed to load keys",t);
  }
}
