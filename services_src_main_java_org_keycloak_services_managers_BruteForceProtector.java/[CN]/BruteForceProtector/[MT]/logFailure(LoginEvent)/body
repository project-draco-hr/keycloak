{
  logger.warn("login failure for user " + event.username + " from ip "+ event.ip);
  failures++;
  long delta=0;
  if (lastFailure > 0) {
    delta=System.currentTimeMillis() - lastFailure;
    if (delta > (long)maxDeltaTimeSeconds * 1000L) {
      totalTime=0;
    }
 else {
      totalTime+=delta;
    }
  }
}
