{
  long currentTime=System.currentTimeMillis();
  Iterator<Entry<String,Long>> itr=expires.entrySet().iterator();
  while (itr.hasNext()) {
    Entry<String,Long> e=itr.next();
    if (e.getValue() < currentTime) {
      itr.remove();
      map.remove(e.getKey());
    }
 else {
      return;
    }
  }
}
