{
  if (values == null) {
    values=new Object[0];
  }
  if ("_id".equals(name)) {
    ObjectId[] objIds=new ObjectId[values.length];
    for (int i=0; i < values.length; i++) {
      String id=values[i].toString();
      ObjectId objectId=new ObjectId(id);
      objIds[i]=objectId;
    }
    values=objIds;
  }
  BasicDBObject inObject=new BasicDBObject("$in",values);
  put(name,inObject);
  return this;
}
