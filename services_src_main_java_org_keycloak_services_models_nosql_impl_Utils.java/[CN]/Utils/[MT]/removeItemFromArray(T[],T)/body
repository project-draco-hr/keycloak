{
  if (item == null) {
    throw new IllegalArgumentException("item must be non-null");
  }
  if (inputArray == null) {
    return inputArray;
  }
 else {
    T[] outputArray=(T[])Array.newInstance(item.getClass(),inputArray.length - 1);
    int counter=0;
    for (    T object : inputArray) {
      if (!item.equals(object)) {
        outputArray[counter++]=object;
      }
    }
    return outputArray;
  }
}
