{
  NoSQLQuery query=NoSQLQueryBuilder.create(MongoDBQueryBuilder.class).andCondition("name",name).andCondition("applicationId",getId()).build();
  RoleData role=noSQL.loadSingleObject(RoleData.class,query);
  if (role == null) {
    return null;
  }
 else {
    return new RoleAdapter(role,noSQL);
  }
}
