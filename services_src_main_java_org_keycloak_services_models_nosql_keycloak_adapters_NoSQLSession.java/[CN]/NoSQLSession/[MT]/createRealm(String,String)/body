{
  if (getRealm(id) != null) {
    throw new IllegalStateException("Realm with id '" + id + "' already exists");
  }
  RealmData newRealm=new RealmData();
  newRealm.setId(id);
  newRealm.setName(name);
  noSQL.saveObject(newRealm);
  RealmAdapter realm=new RealmAdapter(newRealm,noSQL);
  return realm;
}
