{
  UserData userData=((UserAdapter)user).getUser();
  String userId=userData.getId();
  NoSQLQuery query=noSQL.createQueryBuilder().andCondition("socialProvider",socialLink.getSocialProvider()).andCondition("socialUsername",socialLink.getSocialUsername()).andCondition("userId",userId).build();
  noSQL.removeObjects(SocialLinkData.class,query);
}
