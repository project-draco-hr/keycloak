{
  if (auth != null) {
    try {
      require(AccountRoles.MANAGE_ACCOUNT);
    }
 catch (    ForbiddenException e) {
      return Flows.forms(session,realm,null,uriInfo).setError("No access").createErrorPage();
    }
    setReferrerOnPage();
    return account.createResponse(page);
  }
 else {
    return login(path);
  }
}
