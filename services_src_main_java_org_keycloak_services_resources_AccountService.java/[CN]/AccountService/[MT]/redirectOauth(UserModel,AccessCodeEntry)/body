{
  if (accessCode == null) {
    return null;
  }
  Set<RequiredAction> requiredActions=user.getRequiredActions();
  if (!requiredActions.isEmpty()) {
    return Flows.forms(realm,request,uriInfo).setAccessCode(accessCode).setUser(user).forwardToAction(requiredActions.iterator().next());
  }
 else {
    return Flows.oauth(realm,request,uriInfo,authManager,tokenManager).redirectAccessCode(accessCode,accessCode.getState(),accessCode.getRedirectUri());
  }
}
