{
  audit.event(EventType.LOGIN).client(accessCode.getClient()).user(accessCode.getUser()).session(accessCode.getSessionState()).detail(Details.CODE_ID,accessCode.getId()).detail(Details.REDIRECT_URI,accessCode.getRedirectUri()).detail(Details.RESPONSE_TYPE,"code").detail(Details.AUTH_METHOD,accessCode.getAuthMethod()).detail(Details.USERNAME,accessCode.getUsername());
  if (accessCode.isRememberMe()) {
    audit.detail(Details.REMEMBER_ME,"true");
  }
}
