{
  this.auth.requireView();
  IdentityProviderFactory providerFactory=getProviderFactorytById(providerId);
  if (providerFactory != null) {
    event.event(EventType.VIEW_IDENTITY_PROVIDERS).detail(Details.IDENTITY_PROVIDER_FACTORY,providerFactory.getId()).detail(Details.IDENTITY_PROVIDER_FACTORY_IDENTITY,providerFactory.getName()).success();
    return Response.ok(providerFactory).build();
  }
  return Response.status(BAD_REQUEST).build();
}
