{
  auth.requireView();
  Map<String,Integer> stats=new HashMap<String,Integer>();
  for (  ClientModel client : realm.getClients()) {
    int size=session.sessions().getActiveUserSessions(client.getRealm(),client);
    if (size == 0)     continue;
    stats.put(client.getClientId(),size);
  }
  return stats;
}
