{
switch (resource) {
case APPLICATION:
    return AdminRoles.MANAGE_APPLICATIONS;
case CLIENT:
  return AdminRoles.MANAGE_CLIENTS;
case USER:
return AdminRoles.MANAGE_USERS;
case REALM:
return AdminRoles.MANAGE_REALM;
case AUDIT:
return AdminRoles.MANAGE_AUDIT;
default :
throw new IllegalStateException();
}
}
