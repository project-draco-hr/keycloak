{
  auth.requireView();
  RoleModel roleModel=roleContainer.getRole(roleName);
  if (roleModel == null) {
    throw new NotFoundException("Could not find role: " + roleName);
  }
  event.event(EventType.VIEW_ROLE).detail(Details.ROLE_ID,roleModel.getId()).detail(Details.ROLE_NAME,roleModel.getName()).success();
  return getRole(roleModel);
}
