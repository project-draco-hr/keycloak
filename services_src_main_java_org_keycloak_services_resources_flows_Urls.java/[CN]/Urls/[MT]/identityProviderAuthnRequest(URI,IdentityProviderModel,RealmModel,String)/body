{
  UriBuilder uriBuilder=UriBuilder.fromUri(baseURI).path(AuthenticationBrokerResource.class).path(AuthenticationBrokerResource.class,"performLogin").replaceQueryParam("provider_id",identityProvider.getProviderId());
  if (accessCode != null) {
    uriBuilder.replaceQueryParam(OAuth2Constants.CODE,accessCode);
  }
  return uriBuilder.build(realm.getName());
}
