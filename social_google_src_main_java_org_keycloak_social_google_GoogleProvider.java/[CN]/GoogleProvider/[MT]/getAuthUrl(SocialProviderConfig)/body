{
  String state=UUID.randomUUID().toString();
  return AuthRequest.create(state,AUTH_PATH).setQueryParam("client_id",config.getKey()).setQueryParam("response_type",DEFAULT_RESPONSE_TYPE).setQueryParam("scope",DEFAULT_SCOPE).setQueryParam("redirect_uri",config.getCallbackUrl()).setQueryParam("state",state).setAttribute("state",state).build();
}
