{
  String state=UUID.randomUUID().toString();
  callback.putState(state,null);
  return callback.createUri("https://accounts.google.com/o/oauth2/auth").setQueryParam("client_id",callback.getProviderKey()).setQueryParam("response_type","code").setQueryParam("scope","https://www.googleapis.com/auth/userinfo.profile https://www.googleapis.com/auth/userinfo.email").setQueryParam("redirect_uri",callback.getProviderCallbackUrl().toString()).setQueryParam("state",state).build();
}
