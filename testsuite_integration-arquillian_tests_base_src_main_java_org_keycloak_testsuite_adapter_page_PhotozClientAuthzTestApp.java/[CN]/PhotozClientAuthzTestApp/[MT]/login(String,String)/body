{
  navigateTo();
  Thread.sleep(2000);
  if (this.driver.getCurrentUrl().startsWith(getInjectedUrl().toString())) {
    Thread.sleep(2000);
    logOut();
    navigateTo();
  }
  Thread.sleep(2000);
  this.loginPage.form().login(username,password);
  if (this.consentPage.isCurrent()) {
    consentPage.confirm();
    Thread.sleep(2000);
  }
}
