{
  boolean relative=isRelative(testClass.getJavaClass());
  modifyAdapterConfig(archive,ADAPTER_CONFIG_PATH,relative);
  modifyAdapterConfig(archive,ADAPTER_CONFIG_PATH_TENANT1,relative);
