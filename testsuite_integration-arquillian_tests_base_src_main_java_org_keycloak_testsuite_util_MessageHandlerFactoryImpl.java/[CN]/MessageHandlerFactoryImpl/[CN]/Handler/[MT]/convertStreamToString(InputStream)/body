{
  BufferedReader reader=new BufferedReader(new InputStreamReader(is));
  StringBuilder sb=new StringBuilder();
  String line=null;
  try {
    while ((line=reader.readLine()) != null) {
      sb.append(line + "\n");
    }
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  return sb.toString();
}
