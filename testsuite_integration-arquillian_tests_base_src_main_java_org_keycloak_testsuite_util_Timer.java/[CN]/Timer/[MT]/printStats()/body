{
  if (!stats.isEmpty()) {
    System.out.println("OPERATION STATS:");
  }
  for (  String op : stats.keySet()) {
    long sum=0;
    for (    Long t : stats.get(op)) {
      sum+=t;
    }
    System.out.println(MessageFormat.format("Operation {0} average time: {1,number,#} ms",op,sum / stats.get(op).size()));
  }
  stats.clear();
}
