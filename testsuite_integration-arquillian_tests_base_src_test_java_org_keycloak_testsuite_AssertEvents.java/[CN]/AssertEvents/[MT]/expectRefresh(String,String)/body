{
  return expect(EventType.REFRESH_TOKEN).detail(Details.TOKEN_ID,isUUID()).detail(Details.REFRESH_TOKEN_ID,refreshTokenId).detail(Details.REFRESH_TOKEN_TYPE,TokenUtil.TOKEN_TYPE_REFRESH).detail(Details.UPDATED_REFRESH_TOKEN_ID,isUUID()).detail(Details.CLIENT_AUTH_METHOD,ClientIdAndSecretAuthenticator.PROVIDER_ID).session(sessionId);
}
