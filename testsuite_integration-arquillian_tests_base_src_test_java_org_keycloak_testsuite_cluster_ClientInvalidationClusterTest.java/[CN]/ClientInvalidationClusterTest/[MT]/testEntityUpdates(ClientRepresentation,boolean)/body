{
  client.setClientId(client.getClientId() + "_updated");
  client=updateEntity(client,getCurrentFailNode());
  verifyEntityUpdateDuringFailover(client,backendFailover);
  client.setName(client.getName() + "_updated");
  client=updateEntity(client,getCurrentFailNode());
  verifyEntityUpdateDuringFailover(client,backendFailover);
  return client;
}
