{
  return RequestConfig.custom().setSocketTimeout(HTTP_CLIENT_SOCKET_TIMEOUT).setConnectTimeout(HTTP_CLIENT_CONNECT_TIMEOUT).setConnectionRequestTimeout(HTTP_CLIENT_CONNECTION_REQUEST_TIMEOUT).setCookieSpec(CookieSpecs.DEFAULT).setRedirectsEnabled(true).setRelativeRedirectsAllowed(true).setCircularRedirectsAllowed(false).setMaxRedirects(2).build();
}
