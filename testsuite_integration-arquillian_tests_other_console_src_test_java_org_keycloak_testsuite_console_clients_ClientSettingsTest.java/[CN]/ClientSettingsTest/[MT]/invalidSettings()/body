{
  clientsPage.table().createClient();
  createClientPage.form().save();
  assertAlertDanger();
  clientsPage.navigateTo();
  newClient=createClientRep(TEST_CLIENT_ID,OIDC);
  createClient(newClient);
  clientsPage.navigateTo();
  clientsPage.table().createClient();
  createClientPage.form().setClientId(TEST_CLIENT_ID);
  createClientPage.form().save();
  assertAlertDanger();
}
