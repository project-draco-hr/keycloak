{
  return expect(EventType.LOGIN).detail(Details.CODE_ID,isCodeId()).detail(Details.USERNAME,DEFAULT_USERNAME).detail(Details.RESPONSE_TYPE,"code").detail(Details.AUTH_METHOD,"form").detail(Details.REDIRECT_URI,DEFAULT_REDIRECT_URI).detail(Details.CONSENT,Details.CONSENT_VALUE_NO_CONSENT_REQUIRED).session(isUUID());
}
