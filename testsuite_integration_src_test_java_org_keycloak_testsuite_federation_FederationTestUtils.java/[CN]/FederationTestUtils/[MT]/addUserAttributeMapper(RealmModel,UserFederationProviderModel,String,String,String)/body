{
  UserFederationMapperModel mapperModel=KeycloakModelUtils.createUserFederationMapperModel(mapperName,providerModel.getId(),UserAttributeLDAPFederationMapperFactory.PROVIDER_ID,UserAttributeLDAPFederationMapper.USER_MODEL_ATTRIBUTE,userModelAttributeName,UserAttributeLDAPFederationMapper.LDAP_ATTRIBUTE,ldapAttributeName,UserAttributeLDAPFederationMapper.READ_ONLY,"false");
  realm.addUserFederationMapper(mapperModel);
}
