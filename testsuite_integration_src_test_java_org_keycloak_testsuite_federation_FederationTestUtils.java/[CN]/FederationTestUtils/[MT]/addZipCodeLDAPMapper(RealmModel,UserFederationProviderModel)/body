{
  UserFederationMapperModel mapperModel=KeycloakModelUtils.createUserFederationMapperModel("zipCodeMapper",providerModel.getId(),UserAttributeLDAPFederationMapperFactory.PROVIDER_ID,UserAttributeLDAPFederationMapper.USER_MODEL_ATTRIBUTE,"postal_code",UserAttributeLDAPFederationMapper.LDAP_ATTRIBUTE,LDAPConstants.POSTAL_CODE,UserAttributeLDAPFederationMapper.READ_ONLY,"false");
  realm.addUserFederationMapper(mapperModel);
}
