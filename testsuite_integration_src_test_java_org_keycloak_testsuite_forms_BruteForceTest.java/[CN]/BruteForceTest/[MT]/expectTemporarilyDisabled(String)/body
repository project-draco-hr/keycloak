{
  loginPage.open();
  loginPage.login(username,"password");
  loginPage.assertCurrent();
  String src=driver.getPageSource();
  Assert.assertEquals("Invalid username or password.",loginPage.getError());
  events.expectLogin().session((String)null).error(Errors.USER_TEMPORARILY_DISABLED).detail(Details.USERNAME,"test-user@localhost").removeDetail(Details.CONSENT).assertEvent();
}
