{
  long ITERATIONS=10;
  JaxrsClientLogin login=new JaxrsClientLogin();
  long start=System.currentTimeMillis();
  for (int i=0; i < ITERATIONS; i++) {
    System.out.println("*************************");
    login.run();
  }
  long end=System.currentTimeMillis() - start;
  System.out.println("took: " + end);
}
