{
  List<String> l=new LinkedList<>();
  for (  Spi s : ServiceLoader.load(Spi.class)) {
    l.add(fixedLength(s.getName()) + s.isPrivate());
  }
  Collections.sort(l);
  System.out.println(fixedLength("SPI") + "Private");
  System.out.println("-------------------------------------");
  for (  String s : l) {
    System.out.println(s);
  }
}
