{
  initTomcat();
  ProxyServerBuilder builder=new ProxyServerBuilder().addHttpListener(8080,"localhost");
  InputStream is=ProxyTest.class.getResourceAsStream("/keycloak.json");
  AdapterConfig config=KeycloakDeploymentBuilder.loadAdapterConfig(is);
  builder.target("http://localhost:8082").application(config).base("/customer-portal").errorPage("/error.html").constraint("/users/*").role("user").add().constraint("/admins/*").role("admin").add().constraint("/users/permit").permit().add().constraint("/users/deny").deny().add().add();
  proxyServer=builder.build();
  proxyServer.start();
}
