{
  PrintWriter pw=resp.getWriter();
  pw.print("<html>");
  pw.print("<body>");
  pw.print("<form method=\"post\">");
  pw.print("<label for=\"username\">Username</label><input type=\"text\" id=\"username\" name=\"username\" />");
  pw.print("<input type=\"submit\" id=\"submit\" value=\"login\" />");
  pw.print("</form>");
  pw.print("</body>");
  pw.print("</html>");
  pw.flush();
}
