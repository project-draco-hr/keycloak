{
  if (name != null) {
    nameInput.sendKeys(name);
  }
  if (email != null) {
    emailInput.sendKeys(email);
  }
  if (username != null) {
    usernameInput.sendKeys(username);
  }
  if (password != null) {
    passwordInput.sendKeys(password);
  }
  if (passwordConfirm != null) {
    passwordConfirmInput.sendKeys(passwordConfirm);
  }
  submitButton.click();
}
