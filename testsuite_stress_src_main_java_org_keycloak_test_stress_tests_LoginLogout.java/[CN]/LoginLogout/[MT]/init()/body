{
  loginUrl=OIDCLoginProtocolService.authUrl(UriBuilder.fromUri(authServerUrl)).build(realm).toString();
  logoutUrl=OIDCLoginProtocolService.logoutUrl(UriBuilder.fromUri(authServerUrl)).queryParam(OAuth2Constants.REDIRECT_URI,securedResourceUrl).build(realm).toString();
  try {
    webRule.before();
  }
 catch (  Throwable e) {
    throw new RuntimeException(e);
  }
}
