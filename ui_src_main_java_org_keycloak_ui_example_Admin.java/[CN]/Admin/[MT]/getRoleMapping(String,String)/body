{
  List<String> ids=getRoleMapping(new Id(realm,role));
  List<User> users=new LinkedList<User>();
  List<User> realmUsers=getUsers(realm);
  for (  String id : ids) {
    for (    User u : realmUsers) {
      if (u.getUserId().equals(id)) {
        users.add(u);
      }
    }
  }
  return users;
}
